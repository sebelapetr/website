{
	"name": "sebelapetr/website",
	"description": "Source code of my personal portfolio. Written in PHP using Nette framework.",
	"keywords": ["sebelapetr"],
	"type": "project",
	"license": ["MIT"],
	"require": {
		"php": ">= 8.1",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.1",
		"nette/caching": "^3.1",
		"nette/database": "^3.1",
		"nette/di": "^3.0",
		"nette/finder": "^2.5",
		"nette/forms": "^3.1",
		"nette/http": "^3.1",
		"nette/mail": "^3.1",
		"nette/robot-loader": "^3.3",
		"nette/security": "^3.1",
		"nette/utils": "^3.2",
		"latte/latte": "^2.9",
		"tracy/tracy": "^2.8",
		"nextras/orm": "^4.0",
		"nextras/migrations": "^3.1",
		"nextras/dbal": "^4.0",
		"contributte/console": "^0.9.2",
		"contributte/translation": "^0.9.0",
		"ublaboo/datagrid": "^6.9",
		"contributte/comgate": "^0.2.0",
		"moneyphp/money": "4.0.4",
		"apitte/core": "^0.7.1",
		"apitte/middlewares": "^0.7.0",
		"apitte/negotiation": "^0.7.0",
		"apitte/debug": "^0.7.0",
		"oscarotero/psr7-middlewares": "^3.21",
		"justinrainbow/json-schema": "^5.2",
		"contributte/monolog": "^0.5.0",
		"symfony/lock": "^6.1",
		"contributte/http": "^0.4.1",
		"php-curl-class/php-curl-class": "^9.6",
      "ext-simplexml": "*",
		"contributte/deployer-extension": "^3.2"
    },
	"require-dev": {
		"nette/tester": "^2.3",
		"symfony/thanks": "^1",
		"phpstan/phpstan-nette": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"config": {
		"platform": {
			"php": "8.1"
		},
		"allow-plugins": {
			"symfony/thanks": true
		}
	},
	"minimum-stability": "stable"
}
