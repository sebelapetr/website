{{#extend "layout" title="Light Blue HTML5 Dashboard - Maps Google"}}
    {{#content "sidebar"}}
        {{> sidebar active="maps_google"}}
    {{/content}}
    {{#content "content" mode="replace"}}
        <ol class="breadcrumb">
            <li class="breadcrumb-item">YOU ARE HERE</li>
            <li class="breadcrumb-item active">Google Maps</li>
        </ol>
        <h2 class="page-title">Google <span class="fw-semi-bold">Maps</span></h2>
        <div class="row">
            <div class="col-12">
                <section class="widget">
                    <header>
                        <h6>
                            Google Maps <small>Default and customized</small>
                        </h6>
                        <div class="widget-controls">
                            <a data-widgster="expand" title="Expand" href="#" style="display: none;"><i class="la la-angle-up text-white"></i></a>
                            <a data-widgster="collapse" title="Collapse" href="#"><i class="la la-angle-down text-white"></i></a>
                            <a data-widgster="close" title="Close" href="#"><i class="la la-remove text-white"></i></a>
                        </div>
                    </header>
                    <div class="widget-body maps">
                        <div id="gmap" class="content-map"></div>
                    </div>
                </section>
            </div>
        </div>
    {{/content}}
    {{#content "scripts"}}
        <script src="https://maps.google.com/maps/api/js?key=AIzaSyCzLGh9l7VfM67K_8wSxf4rkqoyo_3YXgY"></script>
        <script src="../../node_modules/gmaps/gmaps.js"></script>

        <!-- page specific js -->
        <script src="../js/maps/gmap.js"></script>
    {{/content}}
{{/extend}}
